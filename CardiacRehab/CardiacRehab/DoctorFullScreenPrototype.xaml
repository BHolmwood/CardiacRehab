        </Border>
        <Label Content="MV: Patient 2" HorizontalAlignment="Left" Height="60" Margin="14,52,0,0" VerticalAlignment="Top" Width="165" FontSize="22"/>

        <d3:ChartPlotter x:Name="ecgGraph" Margin="-1,626,0,2">
            <d3:ChartPlotter.HorizontalAxis>
                <d3:HorizontalAxis x:Name="dateAxis4" FontSize="7"/>
            </d3:ChartPlotter.HorizontalAxis>
            <d3:Header FontFamily="Microsoft Uighur"/>
            <d3:VerticalAxisTitle FontFamily="Microsoft Uighur"/>
            <d3:HorizontalAxisTitle FontFamily="Microsoft Uighur"/>
        </d3:ChartPlotter>
        <Image x:Name="hrImage1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Source="Resources/heart.png" Stretch="Uniform" Margin="27,583,957,135" Width="32" Height="31"/>
        <Label x:Name="hrLabel1" Content="HR : " FontWeight="Bold" Margin="56,583,925,137"/>
        <Label x:Name="hrValue1" Content="--" FontWeight="Bold" Margin="100,584,887,136" BorderBrush="Black" Foreground="#FFF31010"/>
        <Label x:Name="hrUnit1" Content="bpm" FontWeight="Bold" RenderTransformOrigin="-0.55,0.538" Margin="118,584,863,138" Foreground="#FFE82222"/>
        <Label x:Name="oxiLabel1" Content="OX : " FontWeight="Bold" Margin="237,583,744,138"/>
        <Image x:Name="oxiImage1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Source="Resources/oxygen.png" Stretch="Uniform" Margin="204,582,780,136" Width="32" Height="31"/>
        <Label x:Name="oxiValue1" Content="--" FontWeight="Bold" Margin="277,583,710,138" Foreground="#FF43C114"/>
        <Label x:Name="oxiSign1" Content="%" FontWeight="Bold" RenderTransformOrigin="-0.55,0.538" Margin="297,585,693,140" Foreground="#FF43C114" Width="26"/>
        <Image x:Name="bpImage1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Source="Resources/bp.png" Stretch="Uniform" Margin="348,580,636,138" Width="32" Height="31"/>
