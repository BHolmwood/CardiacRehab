        <Label x:Name="bpSysValue1" Content="--" FontWeight="Bold" Margin="424,582,551,131" Foreground="#FF1888E6"/>
        <Label x:Name="bpDiaValue1" Content="--" FontWeight="Bold" Margin="460,580,526,133" Foreground="#FF1888E6"/>
        <Label x:Name="bpSysDivSymbol1" Content="/" FontWeight="Bold" RenderTransformOrigin="1.133,0.577" Margin="445,580,545,143" Foreground="#FF1888E6"/>
        <Image x:Name="rpeImage" HorizontalAlignment="Left" Height="31" Margin="503,578,0,0" VerticalAlignment="Top" Width="32" OpacityMask="Black" Source="Resources/rpe.png"/>
        <Label x:Name="rpeValue" Content="--" FontWeight="Bold" Margin="535,582,452,138" BorderBrush="Black" Foreground="#FF0C0B0B"/>

    </Grid>
</Window>
